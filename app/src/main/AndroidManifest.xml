<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fastcampus_kotlin">
    <!-- 이 앱이 인터넷을 사용할 수 있도록 권한을 줌!! -->
    <permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".ContextActivity"></activity>
        <activity android:name=".ResourceActivity" /> <!-- android:theme="@style/Theme.FastCampus_Kotlin"> -->
        <activity android:name=".NullSafetyActivity" />
        <activity android:name=".FragmentActivity" />
        <activity android:name=".InternetOpen" />
        <activity android:name=".Intent_b" />
        <activity android:name=".Intent_a" />
        <activity android:name=".Calculator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Listener" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>